{
  "name": "vue-aurus",
  "version": "0.0.3",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/fojia/vue-aurus"
  },
  "author": {
    "name": "Vladimir Chairkin",
    "url": "https://github.com/fojia"
  },
  "keywords": [
    "vue-aurus",
    "dataviz",
    "vue",
    "d3",
    "charts",
    "swarmplot",
    "sunburst",
    "bar"
  ],
  "main": "dist/vue-aurus.umd.min.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --inline-vue src/index.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "d3": "^7.6.1",
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "babel-eslint": "^10.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "raw-loader": "^4.0.2",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "files": [
    "dist/*",
    "src/*",
    "*.json",
    "*.js"
  ]
}
